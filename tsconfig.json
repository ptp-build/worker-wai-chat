{
	"compilerOptions": {
		"esModuleInterop": true,
		"noEmit": true,
		"module": "esnext",
		"target": "es2020",
		"lib": [
			"es2020",
			"dom"
		],
		"strict": true,
		"alwaysStrict": true,
		"preserveConstEnums": true,
		"moduleResolution": "node",
		"sourceMap": true,
		"types": ["@cloudflare/workers-types","@types/service-worker-mock", "@types/jest"],
		"allowSyntheticDefaultImports": true
	},
	"include": ["src"],
	"exclude": ["node_modules", "dist",
		"tests"
	]
}
